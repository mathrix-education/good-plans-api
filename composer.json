{
  "name": "mathrix-education/good-plans-api",
  "description": "The Laravel Lumen Framework.",
  "version": "0.11.0",
  "keywords": [
    "nuit-info2019",
    "laravel",
    "lumen"
  ],
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Mathieu Bour",
      "email": "mathieu@mathrix.fr",
      "homepage": "https://www.linkedin.com/in/mathieu-bour",
      "role": "Developer"
    },
    {
      "name": "Valentin Pollart",
      "email": "valentin@mathrix.fr",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "barryvdh/laravel-cors": "^0.11.4",
    "laravel/lumen-framework": "^6.0",
    "mathrix-education/lumen-jwt-auth": "2.0.0-beta.1",
    "mathrix-education/lumen-zero": "3.0.0-beta.3",
    "nyholm/psr7": "^1.2",
    "symfony/psr-http-message-bridge": "^1.3"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.4",
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.0",
    "mathrix/openapi-assertions": "0.11.0",
    "mathrix/openapi-processor": "1.0.2"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:mathrix-education/lumen-jwt-auth.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:mathrix-education/lumen-zero.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:mathrix-education/openapi-assertions.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:mathrix-education/openapi-processor.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:mathrix-education/coding-standard.git"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "compile:docs": "openapi-processor --input=docs/index.yaml"
  }
}
